#!/usr/bin/env sh

for arg in "$@"
do
case $arg in
	"--enable-ocamlbuild")
		echo "OCAMLBUILD=enabled" >> Makefile.config
		;;
	"--disable-ocamlbuild") 
		echo "OCAMLBUILD=disabled" >> Makefile.config
		;;
	"--enable-dune")
		echo "DUNE=enabled" >> Makefile.config
		;;
	"--disable-dune") 
		echo "DUNE=disabled" >> Makefile.config
		;;
	*) 
		;;

esac
done

ocaml preprocessor.ml
